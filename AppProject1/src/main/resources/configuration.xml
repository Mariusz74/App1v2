<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="lecturerTest" class="com.group3.domain.Lecturer">
		<constructor-arg name="Name" value="Mary's farm"></constructor-arg>
		<constructor-arg name="ID" value="Mary has a little lamb, her fleece is white as snow"></constructor-arg>
		
</bean>	                                
           
<bean id="deferralTest" class="com.group3.domain.DeferralImpl">
		<constructor-arg name="lecTest" ref="lecturerTest"></constructor-arg>


</bean>	   

</beans>

